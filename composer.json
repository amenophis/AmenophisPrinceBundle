{
    "name": "amenophis/prince-bundle",
    "type": "symfony-bundle",
    "description": "Prince bundle. It integrates Prince XML library into Symfony2 application.",
    "keywords": ["prince", "pdf", "htmltopdf"],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Leherpeur",
            "homepage": "http://github.com/amenophis"
        }
    ],
    "require": {
        "php":                            ">=5.3.3",

        "symfony/framework-bundle":       "~2.3",
        "amenophis/prince-wrapper":       "1.0.*@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Amenophis\\Bundle\\PrinceBundle": "" }
    },
    "target-dir": "Amenophis/Bundle/PrinceBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
